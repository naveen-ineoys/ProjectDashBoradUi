        box-shadow: 0px 0px 1px #171a1f12, 0px 0px 2px #171a1f1F; /* shadow-xs */
    }

    .header{
        background: #DFDFDFFF;
        border-radius: 0px;
        box-shadow: 0px 0px 1px #171a1f12, 0px 0px 2px #171a1f1F;
    }

    .btn-reservation{
        background: #EF7F1BFF;
